            <oneOrMore>
               <ref name="tei_model.pLike"/>
            </oneOrMore>
      
            <group>
        
               <optional>
                  <ref name="tei_summary"/>
               </optional>
        
        
               <oneOrMore>
                  <ref name="tei_layout"/>
