        </zeroOrMore>
      </group>
      <sch:pattern id="myTEI-msIdentifier-msId_minimal-constraint-2">
            <sch:rule context="tei:msIdentifier">
               <sch:report test="not(parent::tei:msPart) and       (local-name(*[1])='idno' or       local-name(*[1])='altIdentifier' or       normalize-space(.)='')">An msIdentifier must contain either a repository or location of some type, or a manuscript name</sch:report>

            </sch:rule>

         </sch:pattern>
